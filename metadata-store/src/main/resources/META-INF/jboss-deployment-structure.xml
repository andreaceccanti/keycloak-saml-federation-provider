<jboss-deployment-structure>
    <deployment>
        <dependencies>
          <module name="org.keycloak.keycloak-common" export="true" />
          <module name="org.keycloak.keycloak-core" export="true" />
          <module name="org.keycloak.keycloak-server-spi"
            export="true" />
          <module name="org.keycloak.keycloak-server-spi-private"
            export="true" />
          <module name="org.keycloak.keycloak-saml-core" />
          <module name="org.keycloak.keycloak-saml-core-public" />
          <module name="org.keycloak.keycloak-services" export="true" />
          <module name="com.google.guava" export="true" />
          <module name="org.jboss.logging" export="true" />
          <module name="org.apache.commons.io" export="true" />
          <module name="org.infinispan" export="true" /> 
        </dependencies>
    </deployment>
    <sub-deployment name="keycloak-saml-metadata-store.jar">
        <module name="org.infinispan.commons" />
              <module name="org.infinispan" />
      </sub-deployment>
    <ear-subdeployments-isolated>false</ear-subdeployments-isolated>
</jboss-deployment-structure>
